package Arrays;

public class ArrayExamples {

	public static void main(String[] args) {
//Array is a Common name given to Group of similar type of values
		//Array is used store group of same tyoe of values
	//int a=10;	
		// TODO Auto-generated method stub
int[] a=new int[4];///Array Declaration 1 2 3 5 7
//Memory 0 1 2 3 
//float [] f=new float[3];
a[0]=1;  //Array initialization
a[1]=2;

System.out.println("first element "+a[0]);  //How to get value from array
System.out.println("third element "+a[2]);

float [] f=new float[3];// array creation
f[0]=3.89f;  //array initialization
float a1=f[0];  //Getting value from array
System.out.println(f[0]);




	}

}
